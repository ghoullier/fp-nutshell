{
  "name": "fp-nutshell",
  "description": "Functional programming in a nutshell",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "run-p 'build:*'",
    "build:bundle": "bun build src/index.ts --outdir ./build --minify",
    "build:types": "tsc -p tsconfig.json"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@tsconfig/node18": "2.0.1",
    "bun-types": "0.5.8",
    "npm-run-all": "4.1.5",
    "typescript": "5.0.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "files": [
      "dist",
      "package.json"
    ]
  }
}
